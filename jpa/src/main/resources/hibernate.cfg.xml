<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD//EN"
                                        "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
 <session-factory>
 
  <property name="hibernate.connection.driver_class">oracle.jdbc.driver.OracleDriver</property>
  <property name="hibernate.connection.url">jdbc:oracle:thin:@localhost:1521:xe</property>
  <property name="hibernate.connection.username">xe</property>
  <property name="hibernate.connection.password">User1</property>
  <property name="dialect">org.hibernate.dialect.Oracle10gDialect</property>
   
   
  <property name="show_sql">true</property>
  <property name="hibernate.hbm2ddl.auto">create</property>
  <mapping class="com.capg.jpa.p3.Examination"/>
  <mapping class="com.capg.jpa.p3.SemExam"/>
   <mapping class="com.capg.jpa.p3.MidExam"/>

  
 
 </session-factory>
</hibernate-configuration>
